<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="UnusedAttribute,ResourceCycle">

    <!--demi light-->
    <font
        android:font="@font/applesdgothicneo_b"
        android:fontStyle="normal"
        android:fontWeight="300"
        app:font="@font/applesdgothicneo_b"
        app:fontStyle="normal"
        app:fontWeight="300" />

    <!--regular-->
    <font
        android:font="@font/applesdgothicneo_m"
        android:fontStyle="normal"
        android:fontWeight="500"
        app:font="@font/applesdgothicneo_m"
        app:fontStyle="normal"
        app:fontWeight="500" />

    <!--medium-->
    <font
        android:font="@font/applesdgothicneo_r"
        android:fontStyle="normal"
        android:fontWeight="1000"
        app:font="@font/applesdgothicneo_r"
        app:fontStyle="normal"
        app:fontWeight="1000" />
</font-family>